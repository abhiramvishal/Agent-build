[project]
name = "{{ project_name }}"
version = "0.1.0"
description = "FastAPI service generated by Project Factory Agent: {{ goal }}"
readme = "README.md"
requires-python = ">= {{ python_version }}"
dependencies = [
    "fastapi>=0.110",
    "uvicorn[standard]>=0.27",
    "httpx>=0.27",
]

[project.optional-dependencies]
dev = [
    "pytest>=7.4",
    "pytest-asyncio>=0.23",
]

[build-system]
requires = ["setuptools>=61.0"]
build-backend = "setuptools.build_meta"

